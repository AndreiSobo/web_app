{
    "routes": [
        {
            "route": "/api/ClassifyPenguinSimple",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/api/XAI",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/classify",
            "methods": [
                "POST"
            ],
            "rewrite": "/api/ClassifyPenguinSimple"
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "platformErrorOverrides": [
        {
            "errorType": "NotFound",
            "serve": "/index.html"
        }
    ]
}